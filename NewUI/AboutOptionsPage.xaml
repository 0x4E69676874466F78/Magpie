<Page x:Class="NewUI.AboutOptionsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:NewUI"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AboutOptionsPage">

    <StackPanel>
        <StackPanel Margin="5,10,0,10" Orientation="Horizontal">
            <Image Height="50" Source="{StaticResource Logo}" HorizontalAlignment="Left" VerticalAlignment="Top"/>
            <Label Content="MAGPIE" Margin="5,0,-4.8,0" VerticalAlignment="Center" FontSize="20"/>
        </StackPanel>


    </StackPanel>
</Page>
